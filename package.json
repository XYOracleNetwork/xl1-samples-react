    "eslint": "~9.34.0",
    "eslint-import-resolver-typescript": "~4.4.4",
    "dotenv": "~17.2.1",
    "npm-check-updates": "~18.0.2",
    "typescript": "~5.9.2",
    "serve": "~14.2.4",
    "vite": "~7.1.3",
    "vite-plugin-top-level-await": "~1.6.0"
  },
  "engines": {
    "node": ">=22.0.0 <23.0.0 || >=24.0.0 <25.0.0"
  },
  "volta": {
    "node": "22.5.1",
    "yarn": "4.9.2",
    "npm": "10.9.2"
